{
  "expo": {
    "name": "Kisscore",
    "slug": "kisscore-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF938E"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF938E"
      },
      "package": "com.cucupapi.kisscoreapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-localization"],
    "extra": {
      "eas": {
        "projectId": "78a83ff1-aa4e-4c29-bfc3-36f0dd6a8e2a"
      }
    }
  },
  "android": {
    "googleServicesFile": "./google-services.json"
  }
}
